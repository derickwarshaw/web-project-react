[ignore]

[include]
./src/*

[libs]

[lints]
all=warn

[options]
include_warnings=true
module.file_ext=.js
module.file_ext=.json
module.file_ext=.css
module.name_mapper.extension='css' -> 'empty/object'
module.file_ext=.scss
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper='^~/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
